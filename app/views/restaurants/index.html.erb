<div class="container">
  <h1>Restaurants</h1>
  <ul style= "list-style: none; padding: 0">
    <% @restaurants.each do |restaurant| %>
      <li><%= link_to restaurant.name, restaurant_path(restaurant) %></li>
    <% end %>
  </ul>
  <%= link_to "Ajouter un nouveau restaurant", new_restaurant_path %>
</div>
